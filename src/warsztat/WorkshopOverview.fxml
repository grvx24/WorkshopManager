<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1024.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="warsztat.WorkshopOverviewController">
  <children>
    <TableView fx:id="tableView" prefHeight="521.0" prefWidth="996.0" AnchorPane.bottomAnchor="185.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="100.0" text="Nazwa" fx:id="nameColumn">
          <cellValueFactory>
            <PropertyValueFactory property="nameColumn" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="100.0" text="Typ" fx:id="typeColumn" />
        <TableColumn prefWidth="150.0" text="Parametry" fx:id="parametersColumn" />
        <TableColumn prefWidth="75.0" text="Kod" fx:id="codeColumn" />
        <TableColumn prefWidth="75.0" resizable="true" text="Ilość" fx:id="quantityColumn" />
        <TableColumn prefWidth="200.0" text="Zastosowanie" fx:id="usageColumn" />
        <TableColumn prefWidth="100.0" text="Inne" fx:id="otherColumn" />
        <TableColumn prefWidth="100.0" text="Brakuje" fx:id="missingColumn" />
      </columns>
      <items>
        <FXCollections fx:factory="observableArrayList" />
      </items>
    </TableView>
    <Button id="addItemButton" mnemonicParsing="false" onAction="#addItemEvent" prefHeight="53.0" prefWidth="196.0" text="Dodaj" AnchorPane.bottomAnchor="132.0" AnchorPane.leftAnchor="14.0">
      <font>
        <Font name="System Bold" size="16.0" fx:id="x1" />
      </font>
    </Button>
    <Button onAction="#deleteSelectedRows" font="$x1" mnemonicParsing="false" prefHeight="53.0" prefWidth="196.0" text="Usuń zaznaczone" AnchorPane.bottomAnchor="132.0" AnchorPane.rightAnchor="14.0" />
  </children>
</AnchorPane>
